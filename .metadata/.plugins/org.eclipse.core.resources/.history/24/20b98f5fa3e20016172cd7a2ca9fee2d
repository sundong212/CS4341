public class Terrain {
	boolean isStart;
	boolean isGoal;
	boolean isUnNav;
	int complexity;
	int row_num;
	int col_num;
	int cost;
	String direction;
	Terrain up;
	Terrain down;
	Terrain left;
	Terrain right;
	Terrain from;
	Terrain to;
	ActionToTake action;
	
	
	public Terrain() {
		this.isStart = false;
		this.isGoal = false;
		this.isUnNav = false;
		this.complexity = -1;
		this.row_num = -1;
		this.col_num = -1;
		this.cost = -1;
		this.direction = null;
		this.up = null;
		this.down = null;
		this.left = null;
		this.right = null;
		this.from = null;
		this.to = null;
		this.action = new ActionToTake();
	}
	
}
