            	buf_line = buf.readLine();
                if(buf_line == null){  
                    break; 
                }else{
                	int current_col = 0;
                	current_line++;
                    wordsArray = buf_line.split("\t");
                    Hashtable<Integer, Terrain> terrains_oneline = new Hashtable<Integer, Terrain>();
                    
                    for(String each : wordsArray){
                    	
                    	current_col++;
                    	
                        if(!"".equals(each)){
                        	
                        	Terrain temp = new Terrain();
                        	
