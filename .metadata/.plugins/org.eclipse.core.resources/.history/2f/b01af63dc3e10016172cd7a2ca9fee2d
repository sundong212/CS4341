								temp.isStart = true;
								temp.row_num = current_line;
								temp.col_num = current_col;
							}
                        	if (each.equals("G")) {
								temp.isGoal = true;
								temp.row_num = current_line;
								temp.col_num = current_col;
							}
                        	if (each.equals("#")) {
								temp.isUnNav
							}
                        	
                        	
                        	
                        	int complexity = Integer.parseInt(each);
                        	
                        	terrains_oneline.add(temp)
                        	
                        }
                        
                        
