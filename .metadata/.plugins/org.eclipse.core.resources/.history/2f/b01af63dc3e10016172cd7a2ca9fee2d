		try{
			String file = args[0];
            BufferedReader buf = new BufferedReader(new FileReader(file));
            Hashtable<Integer, Hashtable<Integer, Terrain>> all_terrains = new Hashtable<Integer, Hashtable<Integer, Terrain>>();
            String buf_line = null;
            String[] wordsArray;
